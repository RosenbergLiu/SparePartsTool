        request.AddParameter("__EVENTARGUMENT", "saveToDisk=format:pdf;showPrintDialog:true;idx:;");
        request.AddParameter("__EVENTTARGET", "ctl00$ContentPlaceHolder1$ReportViewer1");
        request.AddParameter("__LASTFOCUS", "");
